<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
     <body>
     <referenceBlock  name="product.info.details">
        <block class="Magento\Catalog\Block\Product\View\Description" name="product.info.feature" as="feature" template="Magento_Catalog::product/view/attribute.phtml" group="detailed_info">
                    <arguments>
                        <argument name="at_call" xsi:type="string">getTabfeature</argument>
                        <argument name="at_code" xsi:type="string">feature</argument>
                        <argument name="css_class" xsi:type="string">featuredata</argument>
                        <argument name="at_label" xsi:type="string">none</argument>
                        <argument name="title" translate="true" xsi:type="string">feature</argument>
                        <argument name="sort_order" xsi:type="string">11</argument>
                    </arguments>
          </block>
     </referenceBlock>
     </body>
</page>